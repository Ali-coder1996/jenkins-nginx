pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                sh 'sudo ./script.sh'
            }
        }
        stage('Test') { 
            steps {
                sh 'echo "testing"'
            }
        }
        stage('Deploy') { 
            steps {
                sh 'echo "Deploying" '
            }
        }
    }
}